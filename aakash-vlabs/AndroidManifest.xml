<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aakash.vlabs"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.aakash.vlabs.ExpandExp"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.aakash.vlabs.ShowExp"
            android:label="@string/app_name"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.aakash.vlabs.Theory"
            android:label="@string/app_name"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.aakash.vlabs.Procedure"
            android:label="@string/app_name"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.aakash.vlabs.Videos"
            android:label="@string/app_name"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.aakash.vlabs.Simulation"
            android:label="@string/app_name"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.aakash.vlabs.Quiz"
            android:label="@string/app_name"
            android:screenOrientation="landscape" />
        <activity
            android:name="com.aakash.vlabs.Resources"
            android:label="@string/app_name"
            android:screenOrientation="landscape" />
    </application>

</manifest>
