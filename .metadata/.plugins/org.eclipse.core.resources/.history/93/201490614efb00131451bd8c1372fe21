package com.aakash.vlabs.quiz;

public class MyAns {

	// Question Identity
	String Qn,QnType;
	int QnNo;
	float scoredWeight;
	boolean isAnswred;
	
	
	// Case : True / False
	String trueAns;
	String subAns;
	
	// Case : Multiple Choice
	String truemulOptAns;
	String submulOptAns;
	
	// Case : Multiple Many Choice & Short Answer
	String[] truemulManyAns;
	String[] submulManyAns;
	
	// Case : Numeric
	float trueNumeric, trueOffset;
	float subNumeric;
	
	// Case : Matching
	String[] trueMatch;
	String[] subMatch;
	
}
